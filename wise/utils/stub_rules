#!/usr/bin/env python

import sys
from datetime import datetime

data = sys.stdin.readlines()

print '# Autogenerated by Wise annotation utility on', datetime.now()

print '''
try:
    from wise.worksheet.rules import PublicRule
except ImportError:
    raise Exception('Could not load worksheet.rules')
'''

for line in data:
    print "PublicRule('%s')" % line.rstrip()

print '\npanel = {}'
